{
  "format": "custom",
  "id": "news",
  "label": "Notícia",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "cover": {
        "type": "Image",
        "config": {
          "label": "Capa",
          "constraint": {},
          "thumbnails": []
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Titulo",
          "placeholder": ""
        }
      },
      "categoria": {
        "type": "Text",
        "config": {
          "label": "Categoria",
          "placeholder": ""
        }
      },
      "author": {
        "type": "Text",
        "config": {
          "label": "Author",
          "placeholder": ""
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Conteúdo",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      }
